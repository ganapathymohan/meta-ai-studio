<?xml version="1.0" encoding="utf-8"?>
<!--
  Adaptive icon foreground: stylised "M" (for Meta) with
  circuit-node accent dots — all drawn as vector paths.
  Canvas is 108×108dp; safe zone is the inner 72×72dp circle.
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Soft glow circle behind the M -->
    <path
        android:fillColor="#33AAAAFF"
        android:pathData="M54,54
            m-33,0
            a33,33 0,1,0 66,0
            a33,33 0,1,0 -66,0" />

    <!-- White "M" letterform -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="
            M30,74
            L30,36
            L41,36
            L54,57
            L67,36
            L78,36
            L78,74
            L67,74
            L67,52
            L57,68
            L51,68
            L41,52
            L41,74
            Z" />

    <!-- Accent dots (drawn as diamond paths for crisper rendering) -->
    <!-- Top dot -->
    <path
        android:fillColor="#88CCFF"
        android:pathData="M54,22 L57,25 L54,28 L51,25 Z" />
    <!-- Bottom dot -->
    <path
        android:fillColor="#88CCFF"
        android:pathData="M54,80 L57,83 L54,86 L51,83 Z" />
    <!-- Left dot -->
    <path
        android:fillColor="#AA88FF"
        android:pathData="M18,54 L21,57 L18,60 L15,57 Z" />
    <!-- Right dot -->
    <path
        android:fillColor="#AA88FF"
        android:pathData="M90,54 L93,57 L90,60 L87,57 Z" />

    <!-- Thin connector lines -->
    <path
        android:strokeColor="#3388CCFF"
        android:strokeWidth="1"
        android:fillColor="#00000000"
        android:pathData="M54,28 L54,36" />
    <path
        android:strokeColor="#3388CCFF"
        android:strokeWidth="1"
        android:fillColor="#00000000"
        android:pathData="M54,74 L54,80" />
    <path
        android:strokeColor="#3388CCFF"
        android:strokeWidth="1"
        android:fillColor="#00000000"
        android:pathData="M21,57 L30,57" />
    <path
        android:strokeColor="#3388CCFF"
        android:strokeWidth="1"
        android:fillColor="#00000000"
        android:pathData="M78,57 L87,57" />

</vector>
